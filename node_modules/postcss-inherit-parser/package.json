{
  "name": "postcss-inherit-parser",
  "version": "0.1.4",
  "description": "A parser for postcss-inherit's syntax",
  "main": "index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "ava test",
    "coverage": "nyc npm test",
    "codeclimate": "npm run coverage && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarthDB/postcss-inherit-parser.git"
  },
  "keywords": [
    "postcss-syntax",
    "parser",
    "css",
    "postcss"
  ],
  "author": "garthdb <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/postcss-inherit-parser/issues"
  },
  "homepage": "https://github.com/GarthDB/postcss-inherit-parser#readme",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-precompile-charcodes": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "debug": "^2.2.0",
    "postcss": "^5.0.10"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "nyc": "^7.1.0",
    "postcss-merge-rules": "^2.0.10",
    "postcss-merge-selectors": "0.0.6",
    "postcss-parser-tests": "^5.0.9"
  }
}
